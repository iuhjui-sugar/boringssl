From a838c77f0369948f886c54191c4669172ecbba0b Mon Sep 17 00:00:00 2001
From: Bob Beck <bbe@google.com>
Date: Thu, 1 Jun 2023 16:55:10 +0200
Subject: [PATCH 09/15] ensure return from non void functions

---
 net/cert/pki/path_builder.cc                 | 3 +++
 net/cert/pki/simple_path_builder_delegate.cc | 1 +
 net/der/parse_values.cc                      | 1 -
 3 files changed, 4 insertions(+), 1 deletion(-)

diff --git a/net/cert/pki/path_builder.cc b/net/cert/pki/path_builder.cc
index ee2b05930df6e..6013615437ae5 100644
--- a/net/cert/pki/path_builder.cc
+++ b/net/cert/pki/path_builder.cc
@@ -4,6 +4,7 @@
 
 #include "net/cert/pki/path_builder.h"
 
+#include <cassert>
 #include <memory>
 #include <set>
 #include <unordered_set>
@@ -154,6 +155,8 @@ int TrustAndKeyIdentifierMatchToOrder(const ParsedCertificate* target,
           return kDistrustedAndKeyIdMismatch;
       }
   }
+  assert(0);  // NOTREACHED
+  return (-1);
 }
 
 // CertIssuersIter iterates through the intermediates from |cert_issuer_sources|
diff --git a/net/cert/pki/simple_path_builder_delegate.cc b/net/cert/pki/simple_path_builder_delegate.cc
index 8258f7d5d9e3e..83cd265ce78d6 100644
--- a/net/cert/pki/simple_path_builder_delegate.cc
+++ b/net/cert/pki/simple_path_builder_delegate.cc
@@ -80,6 +80,7 @@ bool SimplePathBuilderDelegate::IsSignatureAlgorithmAcceptable(
     case SignatureAlgorithm::kRsaPssSha512:
       return true;
   }
+  return false;
 }
 
 bool SimplePathBuilderDelegate::IsPublicKeyAcceptable(EVP_PKEY* public_key,
diff --git a/net/der/parse_values.cc b/net/der/parse_values.cc
index 1385819f72b3a..0d1769af3ad94 100644
--- a/net/der/parse_values.cc
+++ b/net/der/parse_values.cc
@@ -116,7 +116,6 @@ bool ValidateGeneralizedTime(const GeneralizedTime& time) {
       break;
     default:
       NOTREACHED();
-      return false;
   }
   return true;
 }
